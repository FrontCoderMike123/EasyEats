<div id="logoHolder">
<object data="/images/logo.svg" alt="EasyEats Logo">
<img src="/images/logo.png" alt="EasyEats Logo">
</object>
</div>
<div class="mainTitle">
<h1><%= title %></h1>
<p>Easy as One, Two, Eat!</p>
</div>

<div id="dropDown"></div>

<div id="mobileDropDown">

<div id="userHolder">
    <div id="userPicHolder">
    	<div id="avatarContainer">
    		<object data="/uploads/noUpload/noThumb.svg">
    			<img src="/uploads/noUpload/noThumb.png" alt="No Avatar Yet"/>
    		</object>
    		<img id="uploadedUserImage" src="<%= image %>" alt=""/>
    	</div>
    <article>
        <h3><%= fullName %></h3>
       	<p><%= userName %></p>
        <a href="/profilePicture">Upload Image</a>
    </article>
    </div>
</div>

<div id="settings">
	<a href="/settings">
		<span class="settingMobile">Settings</span>
	</a>
	<a href="/logout">
		<span class="settingMobile">Logout</span>
	</a>
</div>

</div><!--mobile Drop down-->